barrage called swarm	bullet called homer		do action			set speed to 0 over 0.7 seconds			wait 0.7 seconds			set direction to aimed over 0.2 seconds			set speed to 800 over 2 seconds	bullet called seed		speed is 180		do action			set speed to 0 over 60 frames			wait 60 frames			do action				fire homer at absolute speed (50+Math.random()*80) in aimed direction (-180 + (Math.random()-0.5)*2*45)				repeat 8 times			die	action called start		fire seed in aimed direction 90		do action			wait 0.1 seconds			fire seed in incremental direction (180/16) 			repeat 16 times